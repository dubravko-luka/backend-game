{
  "editor.formatOnSave": true,
  "typescript.referencesCodeLens.enabled": true,
  "files.associations": {
    "*.multistage": "dockerfile"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "editor.tabSize": 4,
  "editor.insertSpaces": true
}